```js
import ci from 'ci-info';

switch (ci.name) {
  case 'VERCEL':
    console.log('Running on Vercel');
  case 'GITHUB':
    console.log('Running on Github');
  case 'isCI':
    console.log('Running on CI');
  default:
    console.log('Running locally');
}
```